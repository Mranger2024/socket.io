{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "lib": ["es2015", "dom"],
    "noEmit": false
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}